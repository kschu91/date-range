{
  "name": "kschu91/date-range",
  "description": "A small PHP library to extract date ranges out of a list of dates.",
  "type": "library",
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "authors": [
    {
      "name": "Kevin Schu",
      "email": "kevin.schu@aoe.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Aeq\\DateRange\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aeq\\DateRange\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
